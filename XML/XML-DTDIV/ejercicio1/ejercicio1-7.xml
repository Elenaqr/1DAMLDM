    <!ELEMENT nombre (#PCDATA)> <!-- Elemento 'nombre' -->
    <!ELEMENT apodo (#PCDATA)> <!-- Elemento 'apodo' -->
    <!ELEMENT fechaNacimiento (#PCDATA)> <!-- Elemento 'fechaNacimiento' -->
]>

<musicos>
